  dataType: HUInt64
  abstractionName: Input_NaviOverSpdUnit
  valueEnum:
    0x0: "OFF"
    0x1: "KILOMETER"
    0x2: "MILE"
  matchingTable:
    0x1: 0x1
    0x2: 0x2
    ELSE: 0x0


