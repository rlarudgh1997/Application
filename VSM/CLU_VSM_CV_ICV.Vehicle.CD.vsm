- Blue_Light_Filter:
  type: branch


- Speed_Gauge:
  type: branch


- Speed_Gauge.Output_DisplaySpeedUnderDecimalValue:
  type: branch
  dataType: HUInt64


- Speed_Gauge.Output_DisplaySpeedKphValue:
  type: branch
  dataType: HUInt64


- Speed_Gauge.Output_DisplaySpeedValue:
  type: branch
  dataType: HUInt64


- Speed_Gauge.Output_DisplaySpeedUnit:
  type: branch
  dataType: HUInt64
  valueEnum:
    0x0: "KPH"
    0x1: "MPH"


- Speed_Gauge.Output_DisplaySpeedDefaultUnit:
  type: branch
  dataType: HUInt64
  valueEnum:
    0x0: "KPH"
    0x1: "KMPH"
    0x2: "MPH"


- Blue_Light_Filter.Input_HeadUnitBlueLightLevelStatus:
  type: sensor
  signalName: HU_CLU_BlueLightLvl
  dataType: HUInt64
  abstractionName: Input_HeadUnitBlueLightLevelStatus
  valueEnum:
    0x0: "INVALID"
    0x1: "LEVEL1"
    0x2: "LEVEL2"
    0x3: "LEVEL3"
    0x4: "LEVEL4"
    0x5: "LEVEL5"
    0x6: "LEVEL6"
    0x7: "LEVEL7"
    0x8: "LEVEL8"
    0x9: "LEVEL9"
  matchingTable:
    0x1: 0x1
    0x2: 0x2
    0x3: 0x3
    0x4: 0x4
    0x5: 0x5
    0x6: 0x6
    0x7: 0x7
    0x8: 0x8
    0x9: 0x9
    ELSE: 0x0


- Blue_Light_Filter.Input_HeadUnitBlueLightOnOffStatus:
  type: sensor
  signalName: HU_CLU_BlueLightSta
  dataType: HUInt64
  abstractionName: Input_HeadUnitBlueLightOnOffStatus
  valueEnum:
    0x0: "INVALID"
    0x1: "OFF"
    0x2: "ON"
  matchingTable:
    0x1: 0x1
    0x2: 0x2
    ELSE: 0x0


- Speed_Gauge.Input_NaviOverSpdUnit:
  type: sensor
