- Aux_Battery_Warning:
  type: branch


- Aux_Battery_Warning.Input_AuxBatteryWarnReqFromEMS:
  type: sensor
  signalName: BatteryChargeWarningStatus
  dataType: HUInt64
  abstractionName: Input_AuxBatteryWarnReqFromEMS
  valueEnum:
    0x0: "OFF"
    0x1: "ON"
  matchingTable:
    0x1: 0x1
    ELSE: 0x0


